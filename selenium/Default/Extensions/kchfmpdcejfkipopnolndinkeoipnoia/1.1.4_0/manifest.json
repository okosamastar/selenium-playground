{
   "background": {
      "scripts": [ "spoofer.js", "background.js" ]
   },
   "browser_action": {
      "default_icon": "spoofer.png",
      "default_popup": "popup.html",
      "default_title": "User Agent switcher"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "spoofer_cs.js" ],
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_start"
   } ],
   "default_locale": "en",
   "description": "__MSG_appDesc__",
   "icons": {
      "128": "spoofer-128.png",
      "16": "spoofer-16.png",
      "32": "spoofer-32.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "__MSG_appName__",
   "options_page": "options.html",
   "permissions": [ "storage", "unlimitedStorage", "webRequest", "webRequestBlocking", "\u003Call_urls>" ],
   "storage": {
      "managed_schema": "schema.json"
   },
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.1.4",
   "web_accessible_resources": [ "jquery.js", "spoofer_cs.js" ]
}
