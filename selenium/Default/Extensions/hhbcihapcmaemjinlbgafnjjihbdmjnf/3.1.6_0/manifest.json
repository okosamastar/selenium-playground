{
   "author": "DougaGetter",
   "background": {
      "page": "background.html",
      "persistent": true
   },
   "browser_action": {
      "default_icon": {
         "16": "images/default_16px_normal.png",
         "32": "images/default_16px_normal.png"
      },
      "default_title": "__MSG_action_button_default_title__"
   },
   "content_scripts": [ {
      "js": [ "js/front.js" ],
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_start"
   }, {
      "js": [ "js/internal.js" ],
      "matches": [ "https://www.douga-getter.com/loader.html*site=*" ],
      "run_at": "document_start"
   } ],
   "content_security_policy": "default-src 'self' ; img-src 'self' http://* https://* data: ; script-src 'self' ; object-src 'self' ; style-src 'self' http://* https://* 'unsafe-inline' ; connect-src 'self' http://* https://* ;",
   "default_locale": "ja",
   "description": "__MSG_extension_description__",
   "homepage_url": "https://www.douga-getter.com/",
   "icons": {
      "128": "images/icon_128.png",
      "32": "images/icon_32.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Douga Getter",
   "options_ui": {
      "chrome_style": true,
      "page": "options.html"
   },
   "permissions": [ "contextMenus", "webRequest", "webRequestBlocking", "tabs", "http://*/*", "https://*/*" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "3.1.6",
   "web_accessible_resources": [ "images/*" ]
}
